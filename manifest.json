{
  "manifest_version": 3,
  "name": "Cloudhood",
  "version": "1.0",
  "action": {
    "icon": "logo192.png",
    "default_popup": "popup.html",
    "default_icon": {
      "38": "logo192.png"
    },
    "default_title": "Test extension"
  },
  "icons": {
    "16": "logo192.png",
    "32": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.bundle.js"
  }
}
